# Optimization Configuration for PDF Parsing

# Enable LLM-First Strategy (1-2 calls instead of 50-100)
LLM_FIRST_STRATEGY=true

# Enable enhanced Chinese TOC detection
ENHANCED_TOC_DETECTION=true

# TOC check page limit (pages to scan for TOC)
TOC_CHECK_PAGE_NUM=10

# Enable debug logging for TOC detection
DEBUG_TOC_DETECTION=true

# WebSocket connection timeout (seconds)
WEBSOCKET_TIMEOUT=60

# Enable performance monitoring
ENABLE_PERFORMANCE_MONITORING=true
